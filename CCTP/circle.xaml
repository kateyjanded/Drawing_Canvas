<UserControl x:Class="CCTP.circle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CCTP"
             mc:Ignorable="d" 
             d:DesignHeight="68" d:DesignWidth="68">
    <Grid Name="house" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <Viewbox Name="view" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <Grid Name="grd">
                <Ellipse Name="circleUi" Fill="Transparent" Width="68" Height="68" Stroke="White"/>
            </Grid>
        </Viewbox>
    </Grid>
</UserControl>
